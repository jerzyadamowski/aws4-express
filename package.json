{
  "name": "aws4-express",
  "version": "0.11.1",
  "description": "Express middleware handlers for validation AWS Signature V4",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npx tsc",
    "prepare": "npm run build",
    "lint": "npx eslint './src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build && rimraf dist/tests dist/examples",
    "prettier": "prettier ./src/**/* -c -w",
    "test": "mocha"
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/aws4": "^1.11.6",
    "@types/chai": "^5.2.2",
    "@types/express": "^4.17.22",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.48",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "aws4": "^1.13.2",
    "chai": "^5.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.4.0",
    "mocha": "^10.8.2",
    "prettier": "^3.5.3",
    "sinon": "^18.0.1",
    "supertest": "^7.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "aws4-express",
    "express",
    "middleware",
    "handler",
    "AWS",
    "signature",
    "verify",
    "awsv4"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerzyadamowski/aws4-express.git"
  },
  "author": "Jerzy Adamowski",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/jerzyadamowski/aws4-express/issues"
  },
  "homepage": "https://github.com/jerzyadamowski/aws4-express#readme",
  "prettier": "./prettier",
  "eslintConfig": {
    "extends": "./.eslintrc.js"
  }
}
