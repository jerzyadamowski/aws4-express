  "main": "dist/index.js",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npx tsc",
    "prepare": "npm run build",
    "lint": "npx eslint './src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build && rimraf dist/tests dist/examples",
    "prettier": "prettier ./src/**/* -c -w",
    "test": "mocha"
  },
  "dependencies": {
    "express": "^5.2.1",
    "node": "^24.13.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/aws4": "^1.11.6",
    "@types/chai": "^5.2.3",
    "@types/express": "^5.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.10",
    "@types/sinon": "^21.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "aws4": "^1.13.2",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "sinon": "^21.0.1",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "semver": "^7.6.0",
    "debug": "^4.3.4",
    "diff": "^8.0.3"
  },
  "keywords": [
    "aws4-express",
    "express",
    "middleware",
    "handler",
    "AWS",
    "signature",
    "verify",
    "awsv4"
